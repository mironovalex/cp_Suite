<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New TsdInventpricetmp</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @tsdinventpricetmp.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Inventlocationid: </td>
                <td class="itemValue"><input type="text" name="tsdinventpricetmp[InventLocationId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Itemid: </td>
                <td class="itemValue"><input type="text" name="tsdinventpricetmp[ItemId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Inventsizeid: </td>
                <td class="itemValue"><input type="text" name="tsdinventpricetmp[InventSizeId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Retailprice: </td>
                <td class="itemValue"><input type="text" name="tsdinventpricetmp[RetailPrice]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Salesprice: </td>
                <td class="itemValue"><input type="text" name="tsdinventpricetmp[SalesPrice]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Salesdiscount: </td>
                <td class="itemValue"><input type="text" name="tsdinventpricetmp[SalesDiscount]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Yellowlabel: </td>
                <td class="itemValue"><input type="text" name="tsdinventpricetmp[YellowLabel]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
