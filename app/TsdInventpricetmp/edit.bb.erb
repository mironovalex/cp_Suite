<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @tsdinventpricetmp.object %>
    <h1>Edit <%= @tsdinventpricetmp.InventLocationId%></h1>
    <%= link_to "Delete", :action => :delete, :id => @tsdinventpricetmp.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @tsdinventpricetmp.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Inventlocationid: </td>
              <td class="itemValue"><input type="text" name="tsdinventpricetmp[InventLocationId]" value="<%= @tsdinventpricetmp.InventLocationId %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Itemid: </td>
              <td class="itemValue"><input type="text" name="tsdinventpricetmp[ItemId]" value="<%= @tsdinventpricetmp.ItemId %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Inventsizeid: </td>
              <td class="itemValue"><input type="text" name="tsdinventpricetmp[InventSizeId]" value="<%= @tsdinventpricetmp.InventSizeId %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Retailprice: </td>
              <td class="itemValue"><input type="text" name="tsdinventpricetmp[RetailPrice]" value="<%= @tsdinventpricetmp.RetailPrice %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Salesprice: </td>
              <td class="itemValue"><input type="text" name="tsdinventpricetmp[SalesPrice]" value="<%= @tsdinventpricetmp.SalesPrice %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Salesdiscount: </td>
              <td class="itemValue"><input type="text" name="tsdinventpricetmp[SalesDiscount]" value="<%= @tsdinventpricetmp.SalesDiscount %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Yellowlabel: </td>
              <td class="itemValue"><input type="text" name="tsdinventpricetmp[YellowLabel]" value="<%= @tsdinventpricetmp.YellowLabel %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
