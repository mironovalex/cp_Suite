<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @tsditembarcode2.ItemId %></h1>
    <a href="<%= url_for :action => :show, :id => @tsditembarcode2.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @tsditembarcode2.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @tsditembarcode2.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="tsditembarcode2[ItemId]" class="fieldLabel">Itemid</label>
            <input type="text" id="tsditembarcode2[ItemId]" name="tsditembarcode2[ItemId]" value="<%= @tsditembarcode2.ItemId %>" <%= placeholder( "Itemid" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsditembarcode2[InventSizeId]" class="fieldLabel">Inventsizeid</label>
            <input type="text" id="tsditembarcode2[InventSizeId]" name="tsditembarcode2[InventSizeId]" value="<%= @tsditembarcode2.InventSizeId %>" <%= placeholder( "Inventsizeid" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsditembarcode2[ItemBarCode]" class="fieldLabel">Itembarcode</label>
            <input type="text" id="tsditembarcode2[ItemBarCode]" name="tsditembarcode2[ItemBarCode]" value="<%= @tsditembarcode2.ItemBarCode %>" <%= placeholder( "Itembarcode" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
