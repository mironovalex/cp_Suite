<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @tsditembarcodetemp.ItemId %></h1>
    <a href="<%= url_for :action => :show, :id => @tsditembarcodetemp.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @tsditembarcodetemp.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @tsditembarcodetemp.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="tsditembarcodetemp[ItemId]" class="fieldLabel">Itemid</label>
            <input type="text" id="tsditembarcodetemp[ItemId]" name="tsditembarcodetemp[ItemId]" value="<%= @tsditembarcodetemp.ItemId %>" <%= placeholder( "Itemid" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsditembarcodetemp[InventSizeId]" class="fieldLabel">Inventsizeid</label>
            <input type="text" id="tsditembarcodetemp[InventSizeId]" name="tsditembarcodetemp[InventSizeId]" value="<%= @tsditembarcodetemp.InventSizeId %>" <%= placeholder( "Inventsizeid" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsditembarcodetemp[ItemBarCode]" class="fieldLabel">Itembarcode</label>
            <input type="text" id="tsditembarcodetemp[ItemBarCode]" name="tsditembarcodetemp[ItemBarCode]" value="<%= @tsditembarcodetemp.ItemBarCode %>" <%= placeholder( "Itembarcode" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
