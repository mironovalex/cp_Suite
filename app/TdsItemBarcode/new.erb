<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @tdsitembarcode.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @tdsitembarcode.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="tdsitembarcode[ItemId]" class="fieldLabel">Itemid</label>
            <input type="text" id="tdsitembarcode[ItemId]" name="tdsitembarcode[ItemId]" <%= placeholder("Itemid") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tdsitembarcode[InventSizeId]" class="fieldLabel">Inventsizeid</label>
            <input type="text" id="tdsitembarcode[InventSizeId]" name="tdsitembarcode[InventSizeId]" <%= placeholder("Inventsizeid") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tdsitembarcode[ItemBarCode]" class="fieldLabel">Itembarcode</label>
            <input type="text" id="tdsitembarcode[ItemBarCode]" name="tdsitembarcode[ItemBarCode]" <%= placeholder("Itembarcode") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
