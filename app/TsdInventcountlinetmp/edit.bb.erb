<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @tsd_inventcountlinetmp.object %>
    <h1>Edit <%= @tsd_inventcountlinetmp.JourId%></h1>
    <%= link_to "Delete", :action => :delete, :id => @tsd_inventcountlinetmp.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @tsd_inventcountlinetmp.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Jourid: </td>
              <td class="itemValue"><input type="text" name="tsd_inventcountlinetmp[JourId]" value="<%= @tsd_inventcountlinetmp.JourId %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Itembarcode: </td>
              <td class="itemValue"><input type="text" name="tsd_inventcountlinetmp[ItemBarCode]" value="<%= @tsd_inventcountlinetmp.ItemBarCode %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Itemid: </td>
              <td class="itemValue"><input type="text" name="tsd_inventcountlinetmp[ItemId]" value="<%= @tsd_inventcountlinetmp.ItemId %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Inventsizeid: </td>
              <td class="itemValue"><input type="text" name="tsd_inventcountlinetmp[InventSizeId]" value="<%= @tsd_inventcountlinetmp.InventSizeId %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Countedqty: </td>
              <td class="itemValue"><input type="text" name="tsd_inventcountlinetmp[CountedQty]" value="<%= @tsd_inventcountlinetmp.CountedQty %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
