            <label for="tsd_inventcountlinetmp[ItemBarCode]" class="fieldLabel">Itembarcode</label>
            <input type="text" id="tsd_inventcountlinetmp[ItemBarCode]" name="tsd_inventcountlinetmp[ItemBarCode]" value="<%= @tsd_inventcountlinetmp.ItemBarCode %>" <%= placeholder( "Itembarcode" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsd_inventcountlinetmp[ItemId]" class="fieldLabel">Itemid</label>
            <input type="text" id="tsd_inventcountlinetmp[ItemId]" name="tsd_inventcountlinetmp[ItemId]" value="<%= @tsd_inventcountlinetmp.ItemId %>" <%= placeholder( "Itemid" ) %> />
