<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New TsdReceiptline</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @tsd_receiptline.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Jourid: </td>
                <td class="itemValue"><input type="text" name="tsd_receiptline[JourId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Itemid: </td>
                <td class="itemValue"><input type="text" name="tsd_receiptline[ItemId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Inventsizeid: </td>
                <td class="itemValue"><input type="text" name="tsd_receiptline[InventSizeId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Orderprice: </td>
                <td class="itemValue"><input type="text" name="tsd_receiptline[OrderPrice]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Orderedqty: </td>
                <td class="itemValue"><input type="text" name="tsd_receiptline[OrderedQty]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Receivedqty: </td>
                <td class="itemValue"><input type="text" name="tsd_receiptline[ReceivedQty]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
