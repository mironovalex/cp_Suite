<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New AWSBaskettmp</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @awsbaskettmp.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Inventlocationid: </td>
                <td class="itemValue"><input type="text" name="awsbaskettmp[InventLocationId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Userid: </td>
                <td class="itemValue"><input type="text" name="awsbaskettmp[UserId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Itembarcode: </td>
                <td class="itemValue"><input type="text" name="awsbaskettmp[ItemBarCode]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Itemid: </td>
                <td class="itemValue"><input type="text" name="awsbaskettmp[ItemId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Inventsizeid: </td>
                <td class="itemValue"><input type="text" name="awsbaskettmp[InventSizeId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Printcopies: </td>
                <td class="itemValue"><input type="text" name="awsbaskettmp[PrintCopies]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Linenum: </td>
                <td class="itemValue"><input type="text" name="awsbaskettmp[LineNum]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Yellowlabel: </td>
                <td class="itemValue"><input type="text" name="awsbaskettmp[YellowLabel]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
