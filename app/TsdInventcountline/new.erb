<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @tsd_inventcountline.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @tsd_inventcountline.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="tsd_inventcountline[JourId]" class="fieldLabel">Jourid</label>
            <input type="text" id="tsd_inventcountline[JourId]" name="tsd_inventcountline[JourId]" <%= placeholder("Jourid") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsd_inventcountline[ItemBarCode]" class="fieldLabel">Itembarcode</label>
            <input type="text" id="tsd_inventcountline[ItemBarCode]" name="tsd_inventcountline[ItemBarCode]" <%= placeholder("Itembarcode") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsd_inventcountline[ItemId]" class="fieldLabel">Itemid</label>
            <input type="text" id="tsd_inventcountline[ItemId]" name="tsd_inventcountline[ItemId]" <%= placeholder("Itemid") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsd_inventcountline[InventSizeId]" class="fieldLabel">Inventsizeid</label>
            <input type="text" id="tsd_inventcountline[InventSizeId]" name="tsd_inventcountline[InventSizeId]" <%= placeholder("Inventsizeid") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsd_inventcountline[CountedQty]" class="fieldLabel">Countedqty</label>
            <input type="text" id="tsd_inventcountline[CountedQty]" name="tsd_inventcountline[CountedQty]" <%= placeholder("Countedqty") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
