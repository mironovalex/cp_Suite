<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @tsd_inventcountline.JourId %></h1>
    <a href="<%= url_for :action => :show, :id => @tsd_inventcountline.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @tsd_inventcountline.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @tsd_inventcountline.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="tsd_inventcountline[JourId]" class="fieldLabel">Jourid</label>
            <input type="text" id="tsd_inventcountline[JourId]" name="tsd_inventcountline[JourId]" value="<%= @tsd_inventcountline.JourId %>" <%= placeholder( "Jourid" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsd_inventcountline[ItemBarCode]" class="fieldLabel">Itembarcode</label>
            <input type="text" id="tsd_inventcountline[ItemBarCode]" name="tsd_inventcountline[ItemBarCode]" value="<%= @tsd_inventcountline.ItemBarCode %>" <%= placeholder( "Itembarcode" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsd_inventcountline[ItemId]" class="fieldLabel">Itemid</label>
            <input type="text" id="tsd_inventcountline[ItemId]" name="tsd_inventcountline[ItemId]" value="<%= @tsd_inventcountline.ItemId %>" <%= placeholder( "Itemid" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsd_inventcountline[InventSizeId]" class="fieldLabel">Inventsizeid</label>
            <input type="text" id="tsd_inventcountline[InventSizeId]" name="tsd_inventcountline[InventSizeId]" value="<%= @tsd_inventcountline.InventSizeId %>" <%= placeholder( "Inventsizeid" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsd_inventcountline[CountedQty]" class="fieldLabel">Countedqty</label>
            <input type="text" id="tsd_inventcountline[CountedQty]" name="tsd_inventcountline[CountedQty]" value="<%= @tsd_inventcountline.CountedQty %>" <%= placeholder( "Countedqty" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
