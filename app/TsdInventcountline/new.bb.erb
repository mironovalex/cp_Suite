<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New TsdInventcountline</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @tsd_inventcountline.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Jourid: </td>
                <td class="itemValue"><input type="text" name="tsd_inventcountline[JourId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Itembarcode: </td>
                <td class="itemValue"><input type="text" name="tsd_inventcountline[ItemBarCode]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Itemid: </td>
                <td class="itemValue"><input type="text" name="tsd_inventcountline[ItemId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Inventsizeid: </td>
                <td class="itemValue"><input type="text" name="tsd_inventcountline[InventSizeId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Countedqty: </td>
                <td class="itemValue"><input type="text" name="tsd_inventcountline[CountedQty]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
