<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @tsd_inventcountline.object %>
    <h1>Edit <%= @tsd_inventcountline.JourId%></h1>
    <%= link_to "Delete", :action => :delete, :id => @tsd_inventcountline.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @tsd_inventcountline.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Jourid: </td>
              <td class="itemValue"><input type="text" name="tsd_inventcountline[JourId]" value="<%= @tsd_inventcountline.JourId %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Itembarcode: </td>
              <td class="itemValue"><input type="text" name="tsd_inventcountline[ItemBarCode]" value="<%= @tsd_inventcountline.ItemBarCode %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Itemid: </td>
              <td class="itemValue"><input type="text" name="tsd_inventcountline[ItemId]" value="<%= @tsd_inventcountline.ItemId %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Inventsizeid: </td>
              <td class="itemValue"><input type="text" name="tsd_inventcountline[InventSizeId]" value="<%= @tsd_inventcountline.InventSizeId %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Countedqty: </td>
              <td class="itemValue"><input type="text" name="tsd_inventcountline[CountedQty]" value="<%= @tsd_inventcountline.CountedQty %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
