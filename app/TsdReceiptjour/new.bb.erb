<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New TsdReceiptjour</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @tsd_receiptjour.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Jourid: </td>
                <td class="itemValue"><input type="text" name="tsd_receiptjour[JourId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Inventlocationid: </td>
                <td class="itemValue"><input type="text" name="tsd_receiptjour[InventLocationId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Vendid: </td>
                <td class="itemValue"><input type="text" name="tsd_receiptjour[VendId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Vendname: </td>
                <td class="itemValue"><input type="text" name="tsd_receiptjour[VendName]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Shipdate: </td>
                <td class="itemValue"><input type="text" name="tsd_receiptjour[ShipDate]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Overdelivery: </td>
                <td class="itemValue"><input type="text" name="tsd_receiptjour[OverDelivery]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Deleted: </td>
                <td class="itemValue"><input type="text" name="tsd_receiptjour[Deleted]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Imported: </td>
                <td class="itemValue"><input type="text" name="tsd_receiptjour[Imported]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
