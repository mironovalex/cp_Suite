<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @tsd_inventlocation.object %>
    <h1>Edit <%= @tsd_inventlocation.InventLocationId%></h1>
    <%= link_to "Delete", :action => :delete, :id => @tsd_inventlocation.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @tsd_inventlocation.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Inventlocationid: </td>
              <td class="itemValue"><input type="text" name="tsd_inventlocation[InventLocationId]" value="<%= @tsd_inventlocation.InventLocationId %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Inventloationname: </td>
              <td class="itemValue"><input type="text" name="tsd_inventlocation[InventLoationName]" value="<%= @tsd_inventlocation.InventLoationName %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
