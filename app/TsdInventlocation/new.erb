<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @tsd_inventlocation.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @tsd_inventlocation.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="tsd_inventlocation[InventLocationId]" class="fieldLabel">Inventlocationid</label>
            <input type="text" id="tsd_inventlocation[InventLocationId]" name="tsd_inventlocation[InventLocationId]" <%= placeholder("Inventlocationid") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsd_inventlocation[InventLoationName]" class="fieldLabel">Inventloationname</label>
            <input type="text" id="tsd_inventlocation[InventLoationName]" name="tsd_inventlocation[InventLoationName]" <%= placeholder("Inventloationname") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
