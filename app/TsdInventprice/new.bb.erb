<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New TsdInventprice</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @tsd_inventprice.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Inventlocationid: </td>
                <td class="itemValue"><input type="text" name="tsd_inventprice[InventLocationId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Itemid: </td>
                <td class="itemValue"><input type="text" name="tsd_inventprice[ItemId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Inventsizeid: </td>
                <td class="itemValue"><input type="text" name="tsd_inventprice[InventSizeId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Retailprice: </td>
                <td class="itemValue"><input type="text" name="tsd_inventprice[RetailPrice]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Salesprice: </td>
                <td class="itemValue"><input type="text" name="tsd_inventprice[SalesPrice]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Salesdiscount: </td>
                <td class="itemValue"><input type="text" name="tsd_inventprice[SalesDiscount]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Yellowlabel: </td>
                <td class="itemValue"><input type="text" name="tsd_inventprice[YellowLabel]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
