<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New TsdInventsum</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @tsd_inventsum.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Inventlocationid: </td>
                <td class="itemValue"><input type="text" name="tsd_inventsum[InventLocationId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Itemid: </td>
                <td class="itemValue"><input type="text" name="tsd_inventsum[ItemId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Inventsizeid: </td>
                <td class="itemValue"><input type="text" name="tsd_inventsum[InventSizeId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Vendcode: </td>
                <td class="itemValue"><input type="text" name="tsd_inventsum[VendCode]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Availableqty: </td>
                <td class="itemValue"><input type="text" name="tsd_inventsum[AvailableQty]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Orderedqty: </td>
                <td class="itemValue"><input type="text" name="tsd_inventsum[OrderedQty]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
