          </div>
      
          <div data-role="fieldcontain">
            <label for="tsd_inventcountjour[JourDescription]" class="fieldLabel">Jourdescription</label>
            <input type="text" id="tsd_inventcountjour[JourDescription]" name="tsd_inventcountjour[JourDescription]" value="<%= @tsd_inventcountjour.JourDescription %>" <%= placeholder( "Jourdescription" ) %> />
          </div>
      
          <div data-role="fieldcontain">
