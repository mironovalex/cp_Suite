<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @tsditembarcode.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @tsditembarcode.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="tsditembarcode[ItemId]" class="fieldLabel">Itemid</label>
            <input type="text" id="tsditembarcode[ItemId]" name="tsditembarcode[ItemId]" <%= placeholder("Itemid") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsditembarcode[InventSizeId]" class="fieldLabel">Inventsizeid</label>
            <input type="text" id="tsditembarcode[InventSizeId]" name="tsditembarcode[InventSizeId]" <%= placeholder("Inventsizeid") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsditembarcode[ItemBarCode]" class="fieldLabel">Itembarcode</label>
            <input type="text" id="tsditembarcode[ItemBarCode]" name="tsditembarcode[ItemBarCode]" <%= placeholder("Itembarcode") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
