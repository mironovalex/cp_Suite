<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @tsditembarcode.object %>
    <h1>Edit <%= @tsditembarcode.ItemId%></h1>
    <%= link_to "Delete", :action => :delete, :id => @tsditembarcode.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @tsditembarcode.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Itemid: </td>
              <td class="itemValue"><input type="text" name="tsditembarcode[ItemId]" value="<%= @tsditembarcode.ItemId %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Inventsizeid: </td>
              <td class="itemValue"><input type="text" name="tsditembarcode[InventSizeId]" value="<%= @tsditembarcode.InventSizeId %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Itembarcode: </td>
              <td class="itemValue"><input type="text" name="tsditembarcode[ItemBarCode]" value="<%= @tsditembarcode.ItemBarCode %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
