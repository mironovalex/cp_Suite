<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @tsditembarcode.ItemId %></h1>
    <a href="<%= url_for :action => :show, :id => @tsditembarcode.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @tsditembarcode.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @tsditembarcode.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="tsditembarcode[ItemId]" class="fieldLabel">Itemid</label>
            <input type="text" id="tsditembarcode[ItemId]" name="tsditembarcode[ItemId]" value="<%= @tsditembarcode.ItemId %>" <%= placeholder( "Itemid" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsditembarcode[InventSizeId]" class="fieldLabel">Inventsizeid</label>
            <input type="text" id="tsditembarcode[InventSizeId]" name="tsditembarcode[InventSizeId]" value="<%= @tsditembarcode.InventSizeId %>" <%= placeholder( "Inventsizeid" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsditembarcode[ItemBarCode]" class="fieldLabel">Itembarcode</label>
            <input type="text" id="tsditembarcode[ItemBarCode]" name="tsditembarcode[ItemBarCode]" value="<%= @tsditembarcode.ItemBarCode %>" <%= placeholder( "Itembarcode" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
