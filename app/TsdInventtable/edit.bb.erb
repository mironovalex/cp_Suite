<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @tsd_inventtable.object %>
    <h1>Edit <%= @tsd_inventtable.ItemId%></h1>
    <%= link_to "Delete", :action => :delete, :id => @tsd_inventtable.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @tsd_inventtable.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Itemid: </td>
              <td class="itemValue"><input type="text" name="tsd_inventtable[ItemId]" value="<%= @tsd_inventtable.ItemId %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Itemname: </td>
              <td class="itemValue"><input type="text" name="tsd_inventtable[ItemName]" value="<%= @tsd_inventtable.ItemName %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Vendid: </td>
              <td class="itemValue"><input type="text" name="tsd_inventtable[VendId]" value="<%= @tsd_inventtable.VendId %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Vendname: </td>
              <td class="itemValue"><input type="text" name="tsd_inventtable[VendName]" value="<%= @tsd_inventtable.VendName %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
