<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @tsdinventtabletmp.object %>
    <h1>Edit <%= @tsdinventtabletmp.ItemId%></h1>
    <%= link_to "Delete", :action => :delete, :id => @tsdinventtabletmp.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @tsdinventtabletmp.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Itemid: </td>
              <td class="itemValue"><input type="text" name="tsdinventtabletmp[ItemId]" value="<%= @tsdinventtabletmp.ItemId %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Itemname: </td>
              <td class="itemValue"><input type="text" name="tsdinventtabletmp[ItemName]" value="<%= @tsdinventtabletmp.ItemName %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Vendid: </td>
              <td class="itemValue"><input type="text" name="tsdinventtabletmp[VendId]" value="<%= @tsdinventtabletmp.VendId %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Vendname: </td>
              <td class="itemValue"><input type="text" name="tsdinventtabletmp[VendName]" value="<%= @tsdinventtabletmp.VendName %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
