<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @tsd_userinfo.UserId %></h1>
    <a href="<%= url_for :action => :show, :id => @tsd_userinfo.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @tsd_userinfo.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @tsd_userinfo.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="tsd_userinfo[UserId]" class="fieldLabel">Userid</label>
            <input type="text" id="tsd_userinfo[UserId]" name="tsd_userinfo[UserId]" value="<%= @tsd_userinfo.UserId %>" <%= placeholder( "Userid" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsd_userinfo[UserName]" class="fieldLabel">Username</label>
            <input type="text" id="tsd_userinfo[UserName]" name="tsd_userinfo[UserName]" value="<%= @tsd_userinfo.UserName %>" <%= placeholder( "Username" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsd_userinfo[InventLocationId]" class="fieldLabel">Inventlocationid</label>
            <input type="text" id="tsd_userinfo[InventLocationId]" name="tsd_userinfo[InventLocationId]" value="<%= @tsd_userinfo.InventLocationId %>" <%= placeholder( "Inventlocationid" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsd_userinfo[Password]" class="fieldLabel">Password</label>
            <input type="text" id="tsd_userinfo[Password]" name="tsd_userinfo[Password]" value="<%= @tsd_userinfo.Password %>" <%= placeholder( "Password" ) %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tsd_userinfo[SetPassword]" class="fieldLabel">Setpassword</label>
            <input type="text" id="tsd_userinfo[SetPassword]" name="tsd_userinfo[SetPassword]" value="<%= @tsd_userinfo.SetPassword %>" <%= placeholder( "Setpassword" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
