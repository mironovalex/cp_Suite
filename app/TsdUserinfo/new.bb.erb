<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New TsdUserinfo</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @tsd_userinfo.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Userid: </td>
                <td class="itemValue"><input type="text" name="tsd_userinfo[UserId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Username: </td>
                <td class="itemValue"><input type="text" name="tsd_userinfo[UserName]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Inventlocationid: </td>
                <td class="itemValue"><input type="text" name="tsd_userinfo[InventLocationId]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Password: </td>
                <td class="itemValue"><input type="text" name="tsd_userinfo[Password]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Setpassword: </td>
                <td class="itemValue"><input type="text" name="tsd_userinfo[SetPassword]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
